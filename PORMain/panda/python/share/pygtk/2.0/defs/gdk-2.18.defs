  (parameters
    '("GdkWindow*" "window")
  )
)

(define-function offscreen_window_set_embedder
  (c-name "gdk_offscreen_window_set_embedder")
  (return-type "none")
  (parameters
    '("GdkWindow*" "window")
    '("GdkWindow*" "embedder")
  )
)

(define-function offscreen_window_get_embedder
  (c-name "gdk_offscreen_window_get_embedder")
  (return-type "GdkWindow*")
  (parameters
    '("GdkWindow*" "window")
  )
)



;; From gdkregion.h

